.tags {
  text-align: center;
  list-style-type: none;
  margin-top: .8rem;
  font-family: "Vollkorn", sans-serif; }

/*
post styles
*/
.post a {
  font-size: 1.2rem; }

.post .fade {
  -webkit-transition: all 0.2s ease-out;
  -moz-transition: all 0.2s ease-out;
  transition: all 0.2s ease-out;
  color: #BA90DE;
  text-decoration: none; }
  .post .fade:hover, .post .fade:focus {
    opacity: .6; }

.post-info {
  color: #BA90DE;
  font-family: "EB Garamond", serif;
  font-size: .8rem;
  font-weight: 500;
  letter-spacing: .06rem;
  text-align: center;
  margin-top: .8rem;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: auto; }
  .post-info span {
    font-style: italic; }

.post-title {
  color: #58545D;
  font-family: 'Vollkorn SC';
  letter-spacing: .08rem;
  font-size: 1.8rem;
  text-align: center;
  font-weight: 600;
  margin-bottom: .8rem;
  margin-left: auto;
  margin-right: auto;
  margin-top: auto;
  font-variant: small-caps; }

.post p {
  margin-bottom: .8rem;
  max-width: 100%;
  font-size: 1.2rem; }

.post img {
  margin: 0 auto 0.5rem; }

.post img + em {
  color: #BA90DE;
  display: block;
  font-family: "Vollkorn", sans-serif;
  font-size: 0.9rem;
  font-style: normal;
  text-align: center; }

.post img.emoji {
  display: inline-block;
  left: 0;
  transform: none;
  width: 1rem;
  height: 1rem;
  vertical-align: text-top;
  padding: 0;
  margin: 0; }

.post h2 {
  text-align: center;
  font-weight: 700;
  margin-bottom: -.4rem; }

.post h3 {
  text-align: center;
  font-weight: 600;
  font-size: 1.3rem; }

.post h3,
.post h4 {
  font-family: "Vollkorn", sans-serif; }

.post h4 {
  font-size: 1.2rem;
  text-align: center;
  margin-bottom: -.2rem; }

.post li {
  font-size: 1.2rem;
  list-style-type: none;
  text-align: center; }

.pagination {
  margin: .8rem auto .8rem auto;
  max-width: 100%;
  font-family: "Vollkorn", sans-serif;
  text-align: center;
  list-style: none; }
  .pagination li {
    list-style: none;
    display: inline-block; }
  .pagination .top {
    -webkit-transition: all 0.2s ease-out;
    -moz-transition: all 0.2s ease-out;
    transition: all 0.2s ease-out;
    color: #BA90DE;
    font-family: "EB Garamond", serif;
    font-size: .8rem; }
    .pagination .top:focus .pagination .top:hover {
      opacity: .6; }

/* rules for the code blocks */
pre,
