* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  line-height: 1.5;
  margin: 0;
  padding: 0;
  text-rendering: optimizeLegibility;
  font-kerning: normal; }

html {
  font-size: 62.5%;
  font-family: "EB Garamond", serif;
  color: #58545D;
  overflow-y: scroll;
  height: 100%; }

@media all and (min-width: 700px) {
  html {
    font-size: 1.4rem; } }

@media all and (max-width: 699px) {
  html {
    font-size: 1.125rem; } }

body {
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  max-width: 65vw;
  -webkit-font-smoothing: subpixel-antialiased; }

h1 {
  color: #58545D;
  font-family: "Vollkorn", sans-serif;
  margin-top: .8rem;
  margin-bottom: .8rem; }

h2,
h3,
h4,
h5,
h6 {
  color: #58545D;
  font-family: "Vollkorn", sans-serif;
  margin-top: .8rem;
  font-weight: 600;
  font-variant: all-small-caps; }

a {
  -webkit-transition: all 0.2s ease-out;
  -moz-transition: all 0.2s ease-out;
  transition: all 0.2s ease-out;
  color: #BA90DE;
  text-decoration: none; }
  a:hover, a:focus {
    opacity: .6; }

/* 
items in header.html (site title, main menu, shadow/border) 
*/
.nav-header {
  overflow: auto;
  margin-left: auto;
  margin-right: auto;
  border-bottom: 0.19rem solid #58545D; }
  .nav-header > .headerlink > .title {
    -webkit-transition: all 0.2s ease-out;
    -moz-transition: all 0.2s ease-out;
    transition: all 0.2s ease-out;
    color: #58545D;
    margin: .8rem 0 0 0;
    font-size: 2rem;
    font-weight: 100;
    font-family: "Vollkorn", sans-serif; }
    .nav-header > .headerlink > .title:hover, .nav-header > .headerlink > .title:focus {
      opacity: .6; }

.header-menu > .links {
  display: inline-block;
  padding: 0 .8rem 0 0;
  margin-bottom: .8rem;
  font-family: "Vollkorn", sans-serif;
  letter-spacing: .06rem;
  font-size: 1.4rem; }

blockquote {
  border-left: 0.25rem solid #BA90DE;
  color: #58545D;
  margin: .8rem 0;
  padding: 0 1.25rem 0 1.25rem; }

/*
doublecheck these three if i have image/table problems later
*/
img {
  display: block;
  margin: 0 0 1rem;
  max-width: 100%; }

td {
  vertical-align: top; }

table {
  margin-left: auto;
  margin-right: auto; }

details .toc {
  text-align: left !important;
  margin-bottom: .8rem; }

details > summary {
  width: 15em;
  cursor: pointer;
  list-style: none;
  font: 1.4rem "Vollkorn", sans-serif;
  font-weight: 600; }

details > summary::-webkit-details-marker {
  display: none; }

h-card {
  display: inline-block; }

.mammoth-hcard {
  font-family: "EB Garamond", serif;
  font-size: 1.2rem;
  display: inline-block;
  margin-top: .8rem;
  margin-left: 0;
  margin-right: 0; }
  .mammoth-hcard .u-photo {
    display: inline-block;
    left: 0;
    width: 30px;
    height: 15px;
    transform: none;
    vertical-align: baseline;
    padding: 0;
    margin: 0; }

/*
footer styling and flex boxes for side-by-side contact info/h-card.
not sure why the video css is there.
*/
footer {
  font-family: "EB Garamond", serif;
  border-top: 0.19rem solid #58545D;
  font-size: 1.2rem;
  /* remember that this should be $monospaced*/ }
  footer .flex-container {
    display: flex;
    align-items: baseline;
    justify-content: space-around;
    margin: auto; }
    footer .flex-container .flex-container-1 {
      display: flex;
      flex-direction: row;
      justify-content: space-around;
      font-family: "EB Garamond", serif;
      max-width: 100%;
      height: 100%; }
    footer .flex-container .flex-container-2 {
      display: flex;
      flex-direction: row;
      justify-content: space-around;
      font-family: "EB Garamond", serif;
      max-width: 100%;
      height: 100%; }
  footer .credits {
    text-align: center;
    font-size: .8rem;
    font-family: "EB Garamond", serif;
    margin: auto;
    max-width: 100%; }
  footer span {
    color: #58545D;
    font-family: "EB Garamond", serif; }
  footer span#contact ul {
    list-style: none; }
  footer span.monospace {
