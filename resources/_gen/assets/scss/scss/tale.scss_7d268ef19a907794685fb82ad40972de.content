table, pre code, .container, .nav-container, main p,
main nav,
main div.post-info,
main div.post-details,
main blockquote,
main div.catalogue,
main form#manual-webmention,
main details,
main figure, main h1,
main h2,
main h3,
main h4,
main h5,
main h6, main ul,
main ol, footer, .pagination {
  /* Viewers on smaller screens should have 20% of the screen as whitespace
   * to give them some breathing room
   */
  /* When you're on a huge screen, you don't want to be looking _too_ far
   * across the page, so we should limit this to an arbitrary width of
   * 1100px. Anyone at 1200px will see it a little cramped, but it's not as
   * bad as before!
   */ }
  @media (max-width: 1200px) {
    table, pre code, .container, .nav-container, main p,
    main nav,
    main div.post-info,
    main div.post-details,
    main blockquote,
    main div.catalogue,
    main form#manual-webmention,
    main details,
    main figure, main h1,
    main h2,
    main h3,
    main h4,
    main h5,
    main h6, main ul,
    main ol, footer, .pagination {
      max-width: 80%; } }
  @media (min-width: 1200px) {
    table, pre code, .container, .nav-container, main p,
    main nav,
    main div.post-info,
    main div.post-details,
    main blockquote,
    main div.catalogue,
    main form#manual-webmention,
    main details,
    main figure, main h1,
    main h2,
    main h3,
    main h4,
    main h5,
    main h6, main ul,
    main ol, footer, .pagination {
      max-width: 1100px; } }

